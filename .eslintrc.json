{
    "plugins": [
        "import"
		],
		"extends": "plugin:angular/johnpapa",
		"rules": {
			"semi": "off",
			"no-console": "off",
			"no-debugger": "warn",
			"angular/file-name": "off",
			"angular/function-type": "off",
			"angular/no-run-logic": "off"
		},
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module"
	}
}